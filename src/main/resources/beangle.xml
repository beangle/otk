<?xml version="1.0"?>
<beangle xmlns="http://beangle.github.io/schema/framework"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://beangle.github.io/schema/framework http://beangle.github.io/schema/framework-1.0.xsd">
  <cdi>
    <module class="org.beangle.otk.captcha.web.DefaultModule"/>
    <module class="org.beangle.otk.code.web.DefaultModule"/>
    <module class="org.beangle.otk.doc.web.DefaultModule"/>
    <module class="org.beangle.otk.sns.web.DefaultModule"/>
    <module class="org.beangle.otk.sys.web.DefaultModule"/>
    <module class="org.beangle.otk.lang.web.DefaultModule"/>
    <module class="org.beangle.otk.net.web.DefaultModule"/>
    <module class="org.beangle.otk.security.web.DefaultModule"/>

    <module class="org.beangle.serializer.text.DefaultModule"/>
    <module class="org.beangle.serializer.json.DefaultModule"/>
    <module class="org.beangle.serializer.xml.DefaultModule"/>
  </cdi>
  <mvc>
    <profile package="org.beangle.otk.*.web.action">
      <action suffix="WS" defaultMethod="index"/>
      <url path="/" suffix="" style="seo"/>
      <interceptor name="web.Interceptor.cors"/>
    </profile>
  </mvc>

</beangle>
